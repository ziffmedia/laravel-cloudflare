{
    "name": "ziffmedia/laravel-cloudflare",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "cloudflare"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "cloudflare/sdk": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "ZiffMedia\\LaravelCloudflare\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZiffMedia\\LaravelCloudflare\\CloudflareServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/ziffdavis/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/framework": "^11.20",
        "laravel/nova": "^4.35",
        "laravel/pint": "^1.17"
    }
}
